{

    "manifest_version": 3,
    "name": "QGib",
    "version": "2.0.0",

    "description":
    "QGib by Cucumber:\nHelps you convert the text that you typed from a wrong language to the language that you meant to type in.",

    "icons": {
        "16": "Logos/QGib1616.png",
        "48": "Logos/QGib4848.png",
        "128": "Logos/QGib128128.png"
    },

    "action": {
        "default_title": "Convert text"
    },

    "permissions": [
        "scripting",
        "activeTab"
    ],

    "background": {
        "service_worker":"Background.js"
    }





}